{
    "manifest_version": 0,
    "name": "Universal Like/Dislike",
    "version": "0.1",
    "description": "A universal like and dislike button to be used for websites.",
    "icons":{
        "100": "icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "like this page": "like.js",
        "dislike this page" : "dislike.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "homepage_url" : "https://github.com/julzd/unilikedis"
}